{
    "name": "webid/card-actions",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Webid\\CardActions\\": "src/",
            "Webid\\CardActions\\App\\": "src/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webid\\CardActions\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
