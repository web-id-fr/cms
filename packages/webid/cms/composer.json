{
    "name": "webid/cms",
    "description": "CMS - a simple CMS for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Elise",
            "email": "elise@web-id.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "6.*",
        "laravel/nova": "~2.0|~3.0",
        "spatie/laravel-translatable": "^4.2",
        "infinety-es/nova-filemanager": "^2.6",
        "joedixon/nova-translation": "dev-master",
        "webid/language-tool": "*",
        "webid/nova-translatable": "*",
        "digital-creative/collapsible-resource-manager": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "sensiolabs/security-checker": "^5.0",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Webid\\Cms\\": "src/"
        },
        "files": [
            "src/app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Webid\\Cms\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webid\\Cms\\CmsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./src/nova-components/LanguageTool"
        },
        {
            "type": "path",
            "url": "./src/nova-components/TranslatableTool"
        }
    ]
}
