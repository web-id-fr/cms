{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-menu-tool": "cd nova-components/MenuTool && npm run dev",
        "build-menu-tool-prod": "cd nova-components/MenuTool && npm run prod",
        "build-custom-resource-toolbar": "cd src/nova-components/CustomResourceToolbar && npm i && npm run dev",
        "build-custom-resource-toolbar-prod": "cd src/nova-components/CustomResourceToolbar && npm i && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "cross-env": "^7.0",
        "jquery": "^3.3.1",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.13",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.7",
        "vue-template-compiler": "^2.6.11",
        "vue-toasted": "^1.1.25",
        "webpack-shell-plugin": "^0.5.0"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.26",
        "@fortawesome/free-solid-svg-icons": "^5.12.0",
        "@fortawesome/vue-fontawesome": "^0.1.9",
        "dropzone": "^5.7.0",
        "flag-icon-css": "^3.4.6",
        "vue-flag-icon": "^1.0.6",
        "lang.js": "^1.1.14",
        "vue-toasted": "^1.1.27",
        "vue-multiselect": "^2.1.6"
    }
}
